<interface>
  <template class="CcDurationEditor" parent="GtkWidget">
    <child>
      <object class="CcTimelikeEditor" id="editor">
        <property name="mode">duration</property>
        <property name="minute-increment">15</property>
        <signal name="time-changed" handler="editor_time_changed_cb"/>
      </object>
    </child>
  </template>
</interface>
